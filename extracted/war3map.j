//===========================================================================
function Trig_UnionMoneyEarn_Conditions takes nothing returns boolean
    if ( not ( GetItemTypeId(GetManipulatedItem()) == 'I001' ) ) then
        return false
    endif
    return true
endfunction

function Trig_UnionMoneyEarn_Func001C takes nothing returns boolean
    if ( not ( udg_bool_players_union_money[GetConvertedPlayerId(GetTriggerPlayer())] == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_UnionMoneyEarn_Actions takes nothing returns nothing
    if ( Trig_UnionMoneyEarn_Func001C() ) then
        call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), udg_locale_union_money_received)
    else
        call RemoveItem(GetManipulatedItem())
        call UnitAddItemByIdSwapped('I000', GetTriggerUnit())
        set udg_bool_players_union_money[GetConvertedPlayerId(GetTriggerPlayer())]=true
    endif
endfunction

//===========================================================================
function InitTrig_UnionMoneyEarn takes nothing returns nothing
    set gg_trg_UnionMoneyEarn=CreateTrigger()
    call TriggerRegisterAnyUnitEventBJ(gg_trg_UnionMoneyEarn, EVENT_PLAYER_UNIT_DROP_ITEM)
    call TriggerAddCondition(gg_trg_UnionMoneyEarn, Condition(function Trig_UnionMoneyEarn_Conditions))
    call TriggerAddAction(gg_trg_UnionMoneyEarn, function Trig_UnionMoneyEarn_Actions)
endfunction

